{
  "name": "WEBletonLive",
  "version": "1.0.0",
  "description": "A Web Audio synth/sampler inspired by Ableton Live.",
  "scripts": {
    "build": "webpack --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --open --config webpack-dev.config.js --watch --colors"
  },
  "keywords": [
    "ableton",
    "live",
    "webaudio",
    "es6",
    "javascript"
  ],
  "author": "n_coder}",
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.17.0",
    "node-sass": "^4.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "jquery": "^3.2.1",
    "jquery-knob": "^1.2.11",
    "normalize-scss": "^7.0.0",
    "tone": "^0.11.11"
  }
}
